{
  "name": "emberfire",
  "description": "The officially supported Ember binding for Firebase",
  "version": "2.0.7",
  "author": "Firebase (https://firebase.google.com/)",
  "homepage": "https://github.com/firebase/emberfire/",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember try:testall",
    "legacy": "gulp legacy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firebase/emberfire.git"
  },
  "bugs": {
    "url": "https://github.com/firebase/emberfire/issues"
  },
  "license": "MIT",
  "keywords": [
    "ember",
    "firebase",
    "realtime",
    "ember-addon"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "fastbootDependencies": [
      "firebase"
    ]
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "ember": "global:Ember",
    "ember-data": "global:DS",
    "firebase": "global:firebase"
  },
  "dependencies": {
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-webpack": "^1.0.0",
    "chalk": "^2.0.0",
    "ember-cli-babel": "^6.3.0",
    "firebase": "^4.3.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.6.0",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.15.0-beta.2",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-eslint": "^4.0.0",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-mocha": "^0.12.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-cli-shims": "^1.1.0",
    "ember-data": "~2.15.0-beta.4",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.15.0-beta.2",
    "loader.js": "^4.2.3",
    "babelify": "6.4.0",
    "browserify-shim": "3.8.14",
    "browserify": "14.4.0",
    "codeclimate-test-reporter": "^0.5.0",
    "del": "3.0.0",
    "ember-cli-code-coverage": "^0.4.1",
    "ember-sinon": "0.7.0",
    "glob": "7.1.2",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-header": "1.8.9",
    "gulp-jshint": "2.0.4",
